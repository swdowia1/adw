@model adwWEB.Models.ProductPhoto

@{
    if (Model.LargePhoto != null)
    {
        string imageBase64 = Convert.ToBase64String(Model.LargePhoto);
        string imageSrc = string.Format("data:image/gif;base64,{0}", imageBase64);
        <img src="@imageSrc"  />
    }
}

